# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TradPlusDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TradPlusDemo
	
  #TradPlus SDK必须要有，以下可选
  pod 'TradPlusSDK', '5.0.2'

  #facebook audience network
  pod 'TradPlusSDK/FacebookAdapter', '5.0.2'
  pod 'FBAudienceNetwork'

  #google admob
  pod 'TradPlusSDK/AdMobAdapter', '5.0.2'
  pod 'Google-Mobile-Ads-SDK'
  
  #unity 
  pod 'TradPlusSDK/UnityAdapter', '5.0.2'
  pod 'UnityAds'

  #applovin
  pod 'TradPlusSDK/ApplovinAdapter', '5.0.2'
  pod 'AppLovinSDK'

  #tapjoy 
  pod 'TradPlusSDK/TapjoyAdapter', '5.0.2'
  pod 'TapjoySDK'

  #优量汇
  pod 'TradPlusSDK/GDTMobAdapter', '5.0.2'
  pod 'GDTMobSDK', '4.11.12'

  #穿山甲
  pod 'TradPlusSDK/PangleAdapter', '5.0.2'
  pod 'Bytedance-UnionAD', '3.3.1.5'

  #有道
  pod 'TradPlusSDK/YouDaoAdapter', '5.0.2'
  pod 'YDADSDK', '2.15.3'

end
