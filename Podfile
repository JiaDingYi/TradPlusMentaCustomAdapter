# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TradPlusDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TradPlusDemo
	
  #TradPlus SDK必须要有，以下可选
  pod 'TradPlusSDK', '4.6.7.1'

  #facebook audience network
  pod 'TradPlusSDK/FacebookAdapter', '4.6.7.1'
  pod 'FBAudienceNetwork', '5.10.1'

  #google admob
  pod 'TradPlusSDK/AdMobAdapter', '4.6.7.1'
  pod 'Google-Mobile-Ads-SDK', '7.65.0'
  
  #unity 
  pod 'TradPlusSDK/UnityAdapter', '4.6.7.1'
  pod 'UnityAds', '3.4.8'

  #tapjoy 
  pod 'TradPlusSDK/ApplovinAdapter', '4.6.7.1'
  pod 'AppLovinSDK', '6.14.2'

  #tapjoy 
  pod 'TradPlusSDK/TapjoyAdapter', '4.6.7.1'
  pod 'TapjoySDK', '12.6.1'

end
